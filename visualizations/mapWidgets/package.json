    "copy:images": "cp -r ./source/images dist",
    "copy:leaflet-resources": "cp -r ./node_modules/leaflet/dist/leaflet.css ./node_modules/leaflet/dist/images dist/css",
    "release": "date +%Y%m%d-%H%M > dist/RELEASE.txt",
